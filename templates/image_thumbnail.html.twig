<div class="col-lg-3 col-md-4 col-xs-6">
    <a href="/detail/{{image['id']}}">
        <img class="img-fluid img-thumbnail" src="{{'/images/thumbnail/' ~ image['fileName']}}">
    </a>
    <span class="figure-caption">
        By
        <span>{{image['uploadedByName']}}</span>
        <span class="comment float-right">
            <i class="fa fa-comment-o"></i>({{image['like']}})
        </span>
        <span class="like float-right">
            <i class="fa fa-thumbs-o-up"></i>({{image['comment']}})
        </span>
    </span>
</div>
